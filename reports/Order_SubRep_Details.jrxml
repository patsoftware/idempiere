	<queryString language="SQL">
		<![CDATA[select ol.line, 
COALESCE(p.value, c.name, '') AS code,
COALESCE(p.name,c.name, '') AS name, 
COALESCE(p.description, c.description, '') || case when p.description is not null or c.description is not null 
then '</br>' else '' end || coalesce (ol.description, '') as description,
CASE WHEN ol.m_product_id IS NOT NULL
  THEN ol.qtyentered
  ELSE case when ol.description is not null then  ol.qtyentered
  else null end
END as qta,
uom_trl.Name as uom_name,
uom_trl.uomSymbol as uom_symbol,
ol.priceentered as price,
CASE WHEN ol.m_product_id IS NOT NULL AND $P{IsDiscountPrinted}='Y' THEN ol.discount
  ELSE NULL
END as discount,
ol.linenetamt,ol.datepromised, ol.m_product_id, ol.c_charge_id,
tax_trl.taxindicator
from c_orderline ol
inner join c_order o on o.C_order_id = ol.C_order_id 
inner join C_BPartner bp on bp.C_BPartner_ID = o.C_BPartner_ID
inner join AD_Client cl on cl.AD_Client_ID = o.AD_Client_ID
left outer join c_tax tax on tax.c_tax_id = ol.c_tax_id
left outer join c_tax_trl tax_trl on tax_trl.c_tax_id = tax.c_tax_id 
AND tax_trl.AD_LANGUAGE=COALESCE(bp.AD_Language, cl.AD_Language )
left join m_product p ON ol.m_product_id=p.m_product_id
left join c_charge c ON c.c_charge_id=ol.c_charge_id
LEFT outer join c_uom uom on uom.c_uom_id = ol.c_uom_id
left outer join c_uom_trl uom_trl on uom_trl.c_uom_id = uom.c_uom_id
AND uom_trl.AD_LANGUAGE=COALESCE(bp.AD_Language, cl.AD_Language )
where ol.c_order_id=$P{RECORD_ID}
order by ol.line]]>
